# Variáveis de Ambiente - Backend
# Copie este arquivo para .env e preencha com os valores corretos

# Servidor
PORT=3001
NODE_ENV=development

# Supabase
SUPABASE_URL=sua_url_do_supabase
SUPABASE_ANON_KEY=sua_chave_anonima
SUPABASE_SERVICE_ROLE_KEY=sua_chave_service_role

# Google Drive API
GOOGLE_DRIVE_CLIENT_ID=seu_client_id
GOOGLE_DRIVE_CLIENT_SECRET=seu_client_secret
GOOGLE_DRIVE_REDIRECT_URI=seu_redirect_uri
GOOGLE_DRIVE_REFRESH_TOKEN=seu_refresh_token
GOOGLE_DRIVE_FOLDER_ID=id_pasta_comprovantes

# SAP Business One (Integração)
SAP_B1_BASE_URL=https://seu_servidor_sap:50000/b1s/v1
SAP_B1_COMPANY_DB=nome_empresa
SAP_B1_USERNAME=usuario_sap
SAP_B1_PASSWORD=senha_sap

# WMS Expert (Integração)
WMS_EXPERT_BASE_URL=http://seu_servidor_wms/api
WMS_EXPERT_API_KEY=sua_chave_api_wms

# JWT Secret (para autenticação customizada se necessário)
JWT_SECRET=seu_secret_muito_secreto_aqui

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# Logs
LOG_LEVEL=debug
